{
  "name": "termue",
  "version": "0.0.0",
  "description": "Vue.js renderer for terminal",
  "main": "dist/",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "pnpm run build --watch --on-success=\"node dist/index.js\"",
    "dev:ts": "tsx ./src/index.ts"
  },
  "keywords": [
    "vue",
    "terminal"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-boxes": "^4.0.0",
    "slice-ansi": "^7.1.0",
    "string-width": "^7.1.0",
    "tsup": "^8.0.2",
    "vue": "^3.4.27",
    "wrap-ansi": "^9.0.0",
    "yoga-layout": "^3.0.4"
  },
  "devDependencies": {
    "tsx": "^4.10.5",
    "typescript": "^5.4.5"
  }
}
